{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vaultraider",
  "version": "0.1.0",
  "identifier": "com.jorenvangoethem.vaultraider",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "vaultraider",
        "width": 1200,
        "height": 1000
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc4REFBRjAxN0E1MDVFNUIKUldSYlhsQjZBYS9hZUVpaEZjTWtoM2Z0NlNGWVZ2bUdCbm9haitwN0NUWEc2dGVCaXF3bkpZZmkK",
      "endpoints": [
        "https://github.com/Joren-vanGoethem/VaultRaider/releases/latest/download/latest.json"
      ]
    }
  }
}
